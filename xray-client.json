{
  "log": {
    "loglevel": "none"
  },
  "policy": {
    "levels": {
      "0": {
        "connIdle": 30
      }
    }
  },
  "dns": {
    "servers": [
      "1.1.1.1",
      {
        "address": "5.5.5.5",
        "domains": ["direct.speedtest.tvpg.hu"]
      }
    ]
  },
  "inbounds": [
    {
      "listen": "127.0.0.1",
      "port": 10808,
      "protocol": "socks",
      "settings": {
        "auth": "noauth",
        "udp": true
      },
      "tag": "socks"
    },
    {
      "listen": "127.0.0.1",
      "port": 10809,
      "protocol": "dokodemo-door",
      "settings": {
        "address": "direct.speedtest.tvpg.hu",
        "port": 8080,
        "network": "tcp",
        "followRedirect": false
      },
      "tag": "dokodemo"
    }
  ],
  "outbounds": [
    {
      "mux": {
        "concurrency": 1024,
        "enabled": true,
        "xudpConcurrency": 1024,
        "xudpProxyUDP443": "reject"
      },
      "protocol": "vless",
      "settings": {
        "vnext": [
          {
            "address": "127.0.0.1",
            "port": 10809,
            "users": [
              {
                "encryption": "none",
                "id": "root",
                "level": 0
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "httpupgrade",
        "httpupgradeSettings": {
          "host": "direct.speedtest.tvpg.hu"
        }
      },
      "tag": "proxy"
    },
    {
      "protocol": "dns",
      "settings": {
        "address": "95.138.193.252",
        "port": 32015,
        "network": "tcp"
      },
      "tag": "dns-bypass"
    },
    {
      "protocol": "freedom",
      "settings": {
        "domainStrategy": "UseIP"
      },
      "tag": "direct"
    }
  ],
  "routing": {
    "domainStrategy": "IPIfNonMatch",
    "rules": [
      {
        "type": "field",
        "inboundTag": ["dokodemo"],
        "outboundTag": "direct"
      },
      {
        "type": "field",
        "ip": ["1.1.1.1"],
        "network": "udp",
        "port": 53,
        "outboundTag": "proxy"
      },
      {
        "type": "field",
        "ip": ["5.5.5.5"],
        "network": "udp",
        "port": 53,
        "outboundTag": "dns-bypass"
      }
    ]
  }
}
